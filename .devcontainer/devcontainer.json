{
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"initializeCommand": "mkdir -p ${localWorkspaceFolder}/myAwesomeFolder",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}/myAwesomeFolder",
	"onCreateCommand": "touch  /workspaces/${localWorkspaceFolderBasename}/myAwesomeFolder/onCreate.marker",
	"postCreateCommand": "touch  /workspaces/${localWorkspaceFolderBasename}/myAwesomeFolder/postCreate.marker"
}
